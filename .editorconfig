# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_size = 4
indent_style = space
trim_trailing_whitespace = true

[*.html]
insert_final_newline = false

[*.ts]
insert_final_newline = true


[*.scss]
insert_final_newline = true

[*.md]
max_line_length = off
trim_trailing_whitespace = false
